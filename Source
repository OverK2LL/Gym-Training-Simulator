if game.PlaceId == 9681073174 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Gym Training Simulator", "BloodTheme")
 
    -- MAIN
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")

    MainSection:NewToggle("Auto Swing", "Taps", function(v)
        getgenv().autoswing = v
        while true do
            if not getgenv().autoswing then return end
            wait(0.5)
local args = {
    [1] = "Tapping"
}

game:GetService("ReplicatedStorage").Remotes.Tapping:FireServer(unpack(args))

        end
    end)

MainSection:NewToggle("Auto Train", "Trains - Must be in Training Area", function(v)
    getgenv().autoswing = v
    while true do
        if not getgenv().autoswing then return end
local args = {
    [1] = "Training",
    [2] = workspace.__WORKSPACE.__Interact.Training
}
wait(0.1)
game:GetService("ReplicatedStorage").Remotes.Training:FireServer(unpack(args))
end
end)


-- Rebirths



    local Rebirths = Window:NewTab("Rebirths")
    local RebirthsSection = Rebirths:NewSection("Rebirths")

    RebirthsSection:NewToggle("Auto Rebirth 1", "Rebirths", function(v)
        getgenv().autoswing = v
        while true do
            if not getgenv().autoswing then return end

local args = {
    [1] = {
        [1] = 1
    }
}

game:GetService("ReplicatedStorage").Assets.Events.rebirth:FireServer(unpack(args))
wait(1)
end
end)
