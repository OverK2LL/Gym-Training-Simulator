    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(84.1437531, -18.067585, -43.2826233, -0.394414037, 4.92793291e-08, -0.918932855, 7.88354981e-08, 1, 1.97898054e-08, 0.918932855, -6.46391527e-08, -0.394414037)        
end)

