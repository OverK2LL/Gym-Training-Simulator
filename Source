local Boosts = Window:NewTab("Boosts")
local BoostsSection = Boosts:NewSection("Boosts")

BoostsSection:NewButton("X2 Energy", "Uses Energy Boost", function()

local args = {
    [1] = "Boosts",
    [2] = "x2Energy"
}

game:GetService("ReplicatedStorage").Remotes.RefeshClient:FireServer(unpack(args))

end)

BoostsSection:NewButton("X2 Gems", "Uses Gem Boost", function()

local args = {
    [1] = "Boosts",
    [2] = "x2Gems"
}

game:GetService("ReplicatedStorage").Remotes.RefeshClient:FireServer(unpack(args))


end)

BoostsSection:NewButton("X2 Training", "Uses Training Boost", function()

local args = {
    [1] = "Boosts",
    [2] = "x2Training"
}

game:GetService("ReplicatedStorage").Remotes.RefeshClient:FireServer(unpack(args))


end)



local Misc = Window:NewTab("Misc")
local MiscSection = Misc:NewSection("Misc")

MiscSection:NewButton("Daily Rewards", "Gets Daily Reward", function()
    local args = {
        [1] = "Daily"
    }
    
    game:GetService("ReplicatedStorage").Remotes.RefeshClient:FireServer(unpack(args))
end)

MiscSection:NewSlider("Walkspeed", "Changes the walkspeed", 250, 16, function(v)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
end)

