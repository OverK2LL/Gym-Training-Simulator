end)

TeleportSection:NewButton("Flower Island", "Teleports", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-330.967194, -22.6578541, 5212.95117, -0.0743087754, -2.86655482e-08, -0.997235298, -6.44192211e-09, 1, -2.8265001e-08, 0.997235298, 4.32377467e-09, -0.0743087754)        
end)



local Boosts = Window:NewTab("Boosts")
local BoostsSection = Boosts:NewSection("Boosts")

BoostsSection:NewButton("X2 Energy", "Uses Energy Boost", function()

local args = {
    [1] = "Boosts",
    [2] = "x2Energy"
}

game:GetService("ReplicatedStorage").Remotes.RefeshClient:FireServer(unpack(args))

end)

