local Eggs = Window:NewTab("Eggs")
local EggsSection = Eggs:NewSection("Eggs")





local Eggs; --CREATE GLOBAL VARIABLE OF THE USERS SELECTION TO BE USED ANYWHERE
   EggsSection:NewDropdown("Auto Egg Selection", "Click Correct Egg", {"Basic Egg", "Earth Egg", "Enchanted Egg", "Temple Egg", "Beach Egg", "Lava Egg", "Cyber Egg", "Swamp Egg",}, function(value)--THIS FUNCTION ALLOWS US TO STORE THE USERS SELECTION
        Eggs = value; --STORES THE USERS SELECTION
    end)
    

    EggsSection:NewToggle("Auto Open Egg", "Auto Eggs Selected", function(v)


        ----------LOOPS-----------
        getgenv().autowind = v
            while true do
                if not getgenv().autowind then return end
        ----------LOOPS-----------


        if Eggs == "Basic Egg" then --CHECKS WHAT THE USERS CHOOSED THEN APPLIES WHAT WILL HAPPEND TO THAT DATA

            local args = {
                [1] = "EGG",
                [2] = {
                    [1] = workspace.__WORKSPACE.__Eggs:FindFirstChild("Basic Egg"),
                    [2] = 1,
                    [3] = {}
                }
            }
            
            game:GetService("ReplicatedStorage").Remotes.RefeshClient:FireServer(unpack(args))
                
                wait()
            
            else if Eggs == "Earth Egg" then --ANOTHER EXAMPLE

