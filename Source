local Teleport = Window:NewTab("Teleport")
local TeleportSection = Teleport:NewSection("Teleport")

local Teleport; --CREATE GLOBAL VARIABLE OF THE USERS SELECTION TO BE USED ANYWHERE
TeleportSection:NewDropdown("Select Island", "Teleport", {"Training Center", "Little Island", "Enchanted Island", "Temple Island", "Beach Island", "Volanco Island", "Cyber Island", "Swamp Island",}, function(value)--THIS FUNCTION ALLOWS US TO STORE THE USERS SELECTION
    Teleport = value; --STORES THE USERS SELECTION
    end)
    

    TeleportSection:NewButton("Teleport", "Teleports", function(v)

    if Teleport == "Training Center" then 

    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(84.1437531, -18.067585, -43.2826233, -0.394414037, 4.92793291e-08, -0.918932855, 7.88354981e-08, 1, 1.97898054e-08, 0.918932855, -6.46391527e-08, -0.394414037)        

else if Teleport == "Little Island" then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1604.73181, -22.6579571, -909.715576, 0.549066246, 4.5118334e-08, -0.835778832, 2.87228534e-08, 1, 7.28531049e-08, 0.835778832, -6.4007132e-08, 0.549066246)        

else if Teleport == "Enchanted Island" then

